<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans.xsd ">

<bean id="guitar" class="com.zensar.spring.instruments.Guitar"></bean>
<bean id="piano" class="com.zensar.spring.instruments.Piano"></bean>
<bean id="veena" class="com.zensar.spring.instruments.Veena"></bean>
<bean id="sonu nigam" class="com.zensar.performers.Instrumentalist">
<property name="instrumentSongMap">
<map>
<entry key-ref="veena" value="bekhayali"></entry>
<entry key-ref="piano" value="letmeloveyou"></entry>
<entry key-ref="guitar" value="senorita"></entry>

</map>
</property>
</bean>
 
</beans>
